<h2>Books search for"<%= @word %>"</h2>
<table class="table table-hover table-inverse">
    <% if @range == "User" %>
        <tbody>
            <% @users.each do |user| %>
                <tr>
                    <tb><%= user.name %></tb>
                </tr>
            <% end %>
        </tbody>
    <% else %>
        <tbody>
            <% @books.each do |book| %>
                <tr>
                    <tb><%= book.title %></tb>
                </tr>
            <% end %>
        </tbody>
    <% end %>
</table>